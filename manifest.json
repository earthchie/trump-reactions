{
  "name": "ความรู้สึกของลุงตู่",
  "short_name": "ความรู้สึกของลุงตู่",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "เปลี่ยนหน้าความรู้สึกในเฟซบุคเป็นหน้าลุงตู่แทน",
  "homepage_url": "https://fb.com/earthchie/",
  "default_locale": "en",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "*://*.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "css": [
        "content_script/replace-reactions.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/*.png"
  ]
}
